        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
        jaxb:version="2.1">

    <jaxb:bindings schemaLocation="common.xsd" node="/xsd:schema">
        <jaxb:schemaBindings>
            <jaxb:package name="uk.me.greatorex.jaxbvisitor.common.generated"/>
        </jaxb:schemaBindings>
        <jaxb:globalBindings>
            <xjc:simple/>
        </jaxb:globalBindings>
    </jaxb:bindings>
</jaxb:bindings>